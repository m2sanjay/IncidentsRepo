{
  "expo": {
    "name": "Incident Tracker",
    "slug": "argon-free-react-native",
    "privacy": "public",
    "sdkVersion": "37.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash_.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
	  "bundleIdentifier": "com.ge.sanjay"
    },
	"android": {
      "package": "com.ge.sanjay",
      "versionCode":1,
      "permissions":[],
      "icon": "./assets/icon.png"
    }
  }
}
